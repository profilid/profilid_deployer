{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "proxyServer",
      "image": "profilid/profilid_proxy_server",
      "hostname": "proxyServer",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ]
    },
    {
      "name": "client",
      "image": "profilid/profilid_client",
      "hostname": "client",
      "essential": false,
      "memory": 128,
      "links": ["proxyServer"]
    },
    {
      "name": "auth",
      "image": "profilid/profilid_auth",
      "hostname": "auth",
      "essential": false,
      "memory": 128,
      "links": ["proxyServer"]
    },
    {
      "name": "users",
      "image": "profilid/profilid_users",
      "hostname": "users",
      "essential": false,
      "memory": 128,
      "links": ["proxyServer"]
    },
    {
      "name": "profiles",
      "image": "profilid/profilid_profiles",
      "hostname": "profiles",
      "essential": false,
      "memory": 128,
      "link": ["proxyServer"]
    },
    {
      "name": "usersDb",
      "image": "profilid/profilid_users_db",
      "hostname": "users_db",
      "essential": false,
      "memory": 128
    },
    {
      "name": "profilesDb",
      "image": "profilid/profilid_profiles_db",
      "hostname": "profiles_db",
      "essential": false,
      "memory": 128
    }
  ]
}
