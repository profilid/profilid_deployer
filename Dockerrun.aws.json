{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "profilid/profilid_client",
      "hostname": "client",
      "essential": false,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 3000,
          "containerPort": 3000
        }
      ]
    },


    {
      "name": "proxyServer",
      "image": "profilid/profilid_proxy_server",
      "hostname": "proxyServer",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["client","auth","users", "profiles"]
    }

  ]
}
